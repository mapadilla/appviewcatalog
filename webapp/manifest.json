{
    "_version": "1.59.0",
    "sap.app": {
        "id": "appviewcatalog",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.12.1",
            "toolsId": "a2c25bad-079d-4f20-8f8e-da6c684bf6bb"
        },
        "crossNavigation": {
            "inbounds": {
                "flpappviewcatalog-display": {
                    "semanticObject": "flpappviewcatalog",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            }
        },
        "dataSources": {
            "products-srv": {
                "uri": "localservice/db.data.json",
                "type": "JSON"
            },
            "tiles-srv": {
                "uri": "localservice/tiles.config.json",
                "type": "JSON"
            },
            "mainService": {
                "uri": "V2/Northwind/Northwind.svc/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "appviewcatalog.i18n.i18n"
                }
            },
            "mProduct": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "products-srv"
            },
            "mTile": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "tiles-srv"
            },
            "mNorthwind": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "dataSource": "mainService"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "appviewcatalog.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false,
                "bypassed": {
                    "target": "TargetNotFound"
                }
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMain"
                    ]
                },
                {
                    "name": "ListView",
                    "pattern": "ListView",
                    "target": [
                        "TargetList"
                    ]
                },
                {
                    "name": "FormView",
                    "pattern": "FormView",
                    "target": [
                        "TargetForm"
                    ]
                },
                {
                    "name": "DetailView",
                    "pattern": "DetailView/{productId}",
                    "target": [
                        "TargetDetail"
                    ]
                }
            ],
            "targets": {
                "TargetMain": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Main",
                    "viewName": "Main"
                },
                "TargetList": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "List",
                    "viewName": "List"
                },
                "TargetForm": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Form",
                    "viewName": "Form"
                },
                "TargetDetail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Detail",
                    "viewName": "Detail"
                },
                "TargetNotFound": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "NotFound",
                    "viewName": "NotFound"
                }
            }
        },
        "rootView": {
            "viewName": "appviewcatalog.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "appviewcatalog"
    }
}